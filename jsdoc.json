{
	"opts": {
		"destination": "docs/js",
		"pedantic": true,
		"readme": "README.md",
		"recurse": true,
		"template": "node_modules/jsdoc-wmf-theme"
	},
	"plugins": [
		"node_modules/jsdoc-wmf-theme/plugins/default"
	],
	"source": {
		"include": [ "modules" ],
		"exclude": [ ]
	},
	"templates": {
		"cleverLinks": true,
		"default": {
			"useLongnameInNav": true
		},
		"wmf": {
			"maintitle": "Wikispeech",
			"repository": "https://gerrit.wikimedia.org/g/mediawiki/extensions/Wikispeech",
			"siteMap": {
				"sections": true
			},
			"prefixMap": {
				"mw.": "https://doc.wikimedia.org/mediawiki-core/master/js/{type}.html"
			}
		}
	}
}
